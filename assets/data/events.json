---
# json for upcoming and past events lists on Learn page
---

[
  {% assign events = site.data['dsi-events'] %}
  {% for event in events %}
    {
      "name": {{ event.name | jsonify }},
      "date": {{ event.date | jsonify }},
      "date-iso": {{ event['date-iso'] | jsonify }},
      "time": {{ event.time | jsonify }},
      "location": {{ event.location | jsonify }},
      "description": {{ event.description | jsonify }},
      "link": {{ event.link | jsonify }},
      "image": {{ event.image | jsonify }},
      "image_alt": {{ event.image_alt | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]